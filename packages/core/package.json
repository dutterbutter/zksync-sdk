{
  "name": "@zksync-sdk/core",
  "version": "0.0.1-beta",
  "description": "ZKsync Interop Core: types, ABIs, encoders, registry, and errors (framework-agnostic).",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types.js",
      "types": "./dist/types.d.ts"
    },
    "./errors": {
      "import": "./dist/errors.js",
      "types": "./dist/errors.d.ts"
    },
    "./utils": {
      "import": "./dist/utils.js",
      "types": "./dist/utils.d.ts"
    },
    "./encoding/*": {
      "import": "./dist/encoding/*.js",
      "types": "./dist/encoding/*.d.ts"
    },
    "./abis/*": {
      "import": "./dist/abis/*.js",
      "types": "./dist/abis/*.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p ."
  },
  "dependencies": {
    "@noble/hashes": "^1.8.0"
  },
  "peerDependencies": {},
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dutterbutter/zksync-sdk.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/dutterbutter/zksync-sdk/issues"
  }
}
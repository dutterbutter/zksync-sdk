{
  "name": "@zksync-sdk/ethers",
  "version": "0.0.1-beta",
  "description": "Ethers v6 helpers for ZKsync Interop.",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./actions": {
      "import": "./dist/actions/index.js",
      "types": "./dist/actions/index.d.ts"
    },
    "./remoteCall": {
      "import": "./dist/actions/remoteCall.js",
      "types": "./dist/actions/remoteCall.d.ts"
    },
    "./transfers": {
      "import": "./dist/actions/transfers.js",
      "types": "./dist/actions/transfers.d.ts"
    },
    "./bundle": {
      "import": "./dist/actions/bundle.js",
      "types": "./dist/actions/bundle.d.ts"
    },
    "./finalization": {
      "import": "./dist/actions/finalization.js",
      "types": "./dist/actions/finalization.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p ."
  },
  "peerDependencies": {
    "ethers": "^6.15.0"
  },
  "dependencies": {
    "@zksync-sdk/core": "workspace:*"
  },
  "engines": {
    "node": ">=18.17"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
}
